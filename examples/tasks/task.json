{
    "version": 1,
    "schedule": {
        "type": "simple",
        "interval": "1s"
    },
    "workflow": {
        "collect": {
            "metrics": {
                "/intel/procfs/load/min1": {},
                "/intel/procfs/load/min15": {},
                "/intel/procfs/load/runnable_scheduling": {}
            },
            "config": {
                "/intel/procfs/load": {
                    "proc_path": "/proc"
                }
            },
            "process": [
                {
                    "plugin_name": "passthru",
                    "publish": [
                        {
                            "plugin_name": "file",
                            "config": {
                                "file": "/tmp/published_load"
                            }
                        }
                    ]
                }
            ]
        }
    }
}
